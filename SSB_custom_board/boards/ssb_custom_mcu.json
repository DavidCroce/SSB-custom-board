{
  "build": {
    "core": "arduino",
    "extra_flags": "-DARDUINO_SAMD_ZERO",
    "f_cpu": "48000000L",
    "mcu": "samd21g18a",
    "variant": "arduino_zero"
  },
  "connectivity": [
    "serial",
    "usb"
  ],
  "debug": {
    "jlink_device": "ATSAMD21G18",
    "openocd_target": "at91samdXX",
    "svd_path": "ATSAMD21G18A.svd"
  },
  "frameworks": ["arduino"],
  "name": "ssb_custom_mcu",
  "upload": {
    "maximum_ram_size": 32768,
    "maximum_size": 262144,
    "offset_address": "0x2000",
    "protocol": "sam-ba",
    "protocols": ["sam-ba", "openocd", "jlink"],
    "use_1200bps_touch": true,
    "wait_for_upload_port": true
  },
  "url": "https://github.com/DavidCroce/SSB-custom-board",
  "vendor": "SSB"
}
