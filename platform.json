{
    "name": "SSB",
    "version": "1.0.0",
    "description": "Custom platform for Swiss Solar Boat project",
    "keywords": "swiss, solar, boat, custom",
    "url": "https://github.com/DavidCroce/SSB-custom-board",
    "license": "MIT",
    "frameworks": [
        "arduino",
        "mbed"
    ],
    "platforms": [
        {
            "name": "SSB",
            "board_fqbn": "ssb:ssb_custom_mcu:ssb_custom_mcu",
            "build": {
                "flags": [
                    "-D USE_SPECIAL_FEATURE",
                    "-Os"
                ]
            },
            "variants": [
                "default"
            ]
        }
    ],
    "boards": {
        "ssb_custom_mcu": {
            "build": {
                "core": "arduino",
                "variant": "default"
            },
            "frameworks": [
                "arduino"
            ],
            "upload": {
                "protocols": [
                    "arduino",
                    "stlink",
                    "jlink"
                ],
                "maximum_ram_size": 2048,
                "maximum_size": 8192
            }
        }
    }
}
